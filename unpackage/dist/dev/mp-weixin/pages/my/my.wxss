.recommend {
  display: flex;
  justify-content: space-between;
  background-color: #ffffff;
  padding: 30rpx;
  margin: 20rpx 0 0 0;
}
.recommend .heatshowtyle {
  color: #4dd4a8;
}
.recommend .tancestyle {
  color: #4dd4a8;
}
.recommend .heat {
  width: 225rpx;
  font-size: 30rpx;
  display: flex;
  justify-content: space-between;
  margin-right: 20rpx;
}
.gase {
  padding: 10rpx;
  border-radius: 30rpx;
  border: 1px solid #eaeaea;
  /* 边框样式 */
}
.gase .gamlist {
  width: 100%;
  margin-bottom: 20rpx;
  padding-left: 6rpx;
  display: flex;
  align-items: space-around;
  flex-wrap: wrap;
  box-sizing: border-box;
}
.gase .gamlist .prototal {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
.gase .gamlist .prototal .prodlist {
  margin: 20rpx 0;
  background-color: #ffffff;
  padding-bottom: 20rpx;
  -webkit-column-break-inside: avoid;
          break-inside: avoid;
  border-radius: 20rpx;
}
.gase .gamlist .prototal .prodlist .ductlis {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.gase .gamlist .prototal .prodlist .ductlis .details {
  width: 350rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.gase .gamlist .prototal .prodlist .ductlis .details image {
  margin: 10rpx 0;
  border-top-left-radius: 7px;
  border-top-right-radius: 7px;
  width: 330rpx;
  height: 226rpx;
}
.gase .gamlist .prototal .prodlist .ductlis .textdetail {
  width: 300rpx;
  display: flex;
  flex-direction: column;
  padding: 0 10rpx;
}
.gase .gamlist .prototal .prodlist .ductlis .textdetail .redu image {
  width: 22rpx;
  height: 22rpx;
}
.gase .gamlist .ga-list {
  display: inline-block;
  width: 220rpx;
}
.gase .gamlist .ga-list .ga-img image {
  width: 211rpx;
  height: 160rpx;
}
.gase .gamlist .ga-list .ga-img .ear {
  margin-top: -45rpx;
  margin-left: 14rpx;
  display: flex;
  align-items: center;
  font-size: 22rpx;
  color: #8D8D8D;
}
.gase .gamlist .ga-list .ga-img .ear image {
  width: 30rpx;
  height: 30rpx;
}
.gase .gamlist .ga-list .gase-ads {
  margin-top: 25rpx;
  display: flex;
  flex-direction: column;
}
.contain {
  min-height: 100vh;
  min-width: 100vw;
  background-color: #efefef;
}
.contain .headimg {
  overflow: hidden;
  height: 350rpx;
}
.contain .headimg .popup-boxHead {
  width: 450rpx;
  height: 550rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.contain .headimg .popup-boxHead input {
  text-align: center;
  margin-top: 20rpx;
}
.contain .headimg .box-head {
  width: 450rpx;
  align-items: center;
  margin: 80rpx 35rpx;
  display: flex;
}
.contain .headimg .box-head image {
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
}
.contain .headimg .box-head .headlist {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.contain .headimg .box-head .head-text {
  font-family: 'Times New Roman', Times, serif;
  font-weight: 600;
  font-size: 30rpx;
  margin-top: 20rpx;
  color: #f7f7f7;
}
.contain .headimg .myHead {
  width: 180rpx;
  height: 180rpx;
  border-radius: 50%;
  position: relative;
}
.contain .function {
  border-radius: 20rpx;
  margin: auto;
  margin-top: -50rpx;
  background-color: #ffffff;
  width: 89%;
  padding: 20rpx;
  display: flex;
  flex-direction: column;
}
.contain .function .fun_text {
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
}
.contain .function .fun_text .iconimg {
  display: flex;
  flex-direction: column;
  width: 20%;
  align-items: center;
  margin: 20rpx 15rpx;
}
.contain .function .fun_text .iconimg image {
  width: 65rpx;
  height: 65rpx;
}
.contain .function .text_area {
  width: 80%;
}

